<view class="bg">
  <image class="bg-img" src="/assests/image/shu.jpg" />
</view>
<!-- 标题图片 -->
<view class="header">
  <image class="header-img" src="/assests/icons/manage/manage.png" />
</view>
<scroll-view scroll-y="{{true}}" class="main">
  <block wx:for="{{projectList}}">
    <manage-icon
      project="{{item}}"
      currentDate="{{currentDate}}"
      currentTime="currentTime"
      id="{{index}}"
      bindtap="openNav"
    ></manage-icon>
  </block>
</scroll-view>
<view class="footer">
  <navigator url="/pages/Manage/Detail/Detail?title=发布一个新志愿" class="footer-btn">
    创建一个新志愿
  </navigator>
</view>
